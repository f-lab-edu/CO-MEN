<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.flab.comen.relationship.mapper.CoachingRelationshipMapper">


    <select id="findByMenteeIdAndActiveYn" parameterType="Long"
            resultType="com.flab.comen.relationship.domain.CoachingRelationship">
        SELECT *
        FROM coaching_relationship
        WHERE mentee_id = #{menteeId}
          AND active_yn = 'Y'
    </select>

</mapper>
