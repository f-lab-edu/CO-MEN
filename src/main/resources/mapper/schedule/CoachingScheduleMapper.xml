<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.flab.comen.schedule.mapper.CoachingScheduleMapper">


    <select id="findByMenteeIdAndPossibleDt" parameterType="map"
            resultType="com.flab.comen.schedule.domain.CoachingSchedule">
        SELECT *
        FROM coaching_schedule
        WHERE mentee_id = #{menteeId}
          AND possible_dt LIKE #{date} '%'
    </select>

</mapper>
